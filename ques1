#include <iostream>
using namespace std;

class Node {
public:
    int data;
    Node *next;
    Node *prev;
    Node(int value) {
        data = value;
        next = NULL;
        prev = NULL;
    }
};

class CircularList {
    Node *head;
public:
    CircularList() { head = NULL; }

    void insertAtBeginning(int value) {
        Node *newNode = new Node(value);
        if (head == NULL) {
            head = newNode;
            newNode->next = head;
        } else {
            Node *temp = head;
            while (temp->next != head) temp = temp->next;
            temp->next = newNode;
