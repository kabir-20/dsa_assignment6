#include <iostream>
using namespace std;

class LinkedList {
    struct Node {
        int data;
        Node* next;
    };
    Node* head;

public:
    LinkedList() { head = NULL; }

    void create(int n) {
        Node* temp = NULL;
        for (int i = 0; i < n; i++) {
            int val;
            cin >> val;
            Node* newNode = new Node{val, NULL};
            if (head == NULL) {
                head = newNode;
                temp = head;
            } else {
                temp->next = newNode;
                temp = newNode;
            }
        }
        int choice;
        cin >> choice;
        if (choice == 1) temp->next = head;
    }

    void checkCircular() {
        if (head == NULL) {
            cout << "List is empty";
            return;
        }
        Node *slow = head, *fast = head;
        while (fast && fast->next) {
            slow = slow->next;
            fast = fast->next->next;
            if (slow == fast) {
                cout << "The linked list is circular";
                return;
            }
        }
        cout << "The linked list is not circular";
    }
};

int main() {
    LinkedList list;
    int n;
    cin >> n;
    list.create(n);
    list.checkCircular();
    return 0;
}
